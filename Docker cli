   35  sudo apt-get install     ca-certificates     curl     gnupg
   36  sudo install -m 0755 -d /etc/apt/keyrings
   37  curl -fsSL https://download.docker.com/linux/ubuntu/gpg | sudo gpg --dearmor -o /etc/apt/keyrings/docker.gpg
